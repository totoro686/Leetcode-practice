class Solution {
    public int[][] kClosest(int[][] points, int K) {
        if(points.length == 0) return null;
        int[][] result = new int[K][2];
        Arrays.sort(points, (a, b) -> (a[0]*a[0] + a[1]*a[1]) - (b[0]*b[0] + b[1]*b[1]));
        
        for(int i = 0; i < K; i++){
            result[i] = points[i];
        }
        
        return result;
    }
}
